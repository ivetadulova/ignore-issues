  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:check": "tsc --noEmit",
    "build:analyze": "find build -name \"*.js\" | grep -v -e reload -e runtime-main | xargs -n1 -I{} /bin/sh -c 'yarn run source-map-explorer {} {}.map'",
    "test": "react-scripts test --runInBand",
    "eject": "react-scripts eject",
    "lint": "ERROR_STATUS=0 ; yarn lint:js || ERROR_STATUS=$? ; yarn lint:yaml || ERROR_STATUS=$? ; yarn lint:sh || ERROR_STATUS=$? ;echo \"=========\"; exit $ERROR_STATUS",
    "lint:sh": "ERROR_STATUS=0 ; for file in $(find . -type f \\( -iname \"*.sh\" \\) -not -path './node_modules/*' -not -path './.git/*'); do yarn run shellcheck -x $file || ERROR_STATUS=$? ; done; exit $ERROR_STATUS",
    "lint:yaml": "yarn run yamllint \"**/*.yml\" \"**/*.yaml\" --ignore=\"node_modules/**/*.yml\" --ignore=\"node_modules/**/*.yaml\" --ignore=.config/staging/.drone.yml --ignore=.config/production/.drone.yml",
    "lint:js": "eslint ./src --ext .js,.jsh,.jsx,.ts,.tsx",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:fix": "yarn lint:js:fix",
    "lint:js:watch": "esw -w ./src --ext .js,.jsh,.jsx,.ts,.tsx",
    "lint:js:stats": "yarn lint:js ----format node_modules/eslint-stats/byError.js",
    "lint:changed:fix": "./utils/lint-changed-files.sh",
    "lcs": "yarn lint:changed:fix origin/main",
    "ts:check": "yarn build:check",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
